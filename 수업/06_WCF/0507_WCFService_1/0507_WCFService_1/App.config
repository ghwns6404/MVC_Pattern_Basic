<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
	</startup>


	<!--나중에 이거 가져다가 이름&주소 바꾸기-->
	<system.serviceModel>
		<services>
			<service name="_0507_WCFService_1.HelloWorldWCFService"   behaviorConfiguration="HelloWorldService">
				<host>
					<baseAddresses>
						<add baseAddress="http://localhost/wcf/example/helloworldservice" />
					</baseAddresses>
				</host>
				<endpoint contract="_0507_WCFService_1.IHelloWorld"
                          address=""
                          binding="basicHttpBinding" />
			</service>
		</services>
		<behaviors>
			<serviceBehaviors>
				<behavior name="HelloWorldService">
					<serviceMetadata httpGetEnabled="true" />
				</behavior>
			</serviceBehaviors>
		</behaviors>

	</system.serviceModel>
	
	
	
	
</configuration>
