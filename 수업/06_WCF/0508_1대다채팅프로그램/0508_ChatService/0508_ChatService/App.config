<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
	<appSettings>
		<add key="addr" value="net.tcp://220.90.180.111:8000/Chating"/>
	</appSettings>
	<system.serviceModel>
		<services>
			<service name="Chating.ChatService" behaviorConfiguration="FileServiceBehavior">
				<host>
					<baseAddresses>
						<add baseAddress="http://220.90.180.111:9000/GetService"/>
					</baseAddresses>
				</host>
				<endpoint address="" binding="netTcpBinding" bindingConfiguration="DuplexBinding"
					  contract="Chating.IChat" />
			</service>
		</services>
		<bindings>
			<netTcpBinding>
				<binding name="DuplexBinding" maxBufferSize="2147483647" maxConnections="100" maxBufferPoolSize="524288"
				  maxReceivedMessageSize="2147483647">
					<reliableSession enabled="true" />
					<security mode="None" />
				</binding>
			</netTcpBinding>
		</bindings>
		<behaviors>
			<serviceBehaviors>
				<behavior name="FileServiceBehavior">
					<serviceMetadata httpGetEnabled="true"/>
					<serviceDebug includeExceptionDetailInFaults="true"/>
				</behavior>
			</serviceBehaviors>
		</behaviors>
	</system.serviceModel>
</configuration>